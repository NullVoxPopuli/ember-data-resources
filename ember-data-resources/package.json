{
  "name": "ember-data-resources",
  "version": "3.0.6",
  "description": "Resource helpers for reactively (re)fetching data with ember-data",
  "repository": "https://github.com/NullVoxPopuli/ember-data-resources",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "addon-main.cjs",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "concurrently 'npm:watch:*'",
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup -c ./rollup.config.js",
    "build:docs": "cp ../README.md ./README.md",
    "watch:js": "rollup -c --watch --no-watch.clearScreen",
    "lint": "concurrently 'npm:lint:js'",
    "lint:fix": "concurrently 'npm:lint:js:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "@embroider/addon-shim": "1.2.0",
    "ember-resources": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.2",
    "@babel/plugin-syntax-decorators": "7.17.0",
    "@babel/plugin-transform-typescript": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "@embroider/addon-dev": "1.2.0",
    "@nullvoxpopuli/eslint-configs": "2.2.0",
    "@types/ember__destroyable": "^4.0.0",
    "babel-eslint": "10.1.0",
    "concurrently": "7.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-decorator-position": "4.0.1",
    "eslint-plugin-ember": "10.5.9",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "rollup": "2.68.0",
    "rollup-plugin-ts": "2.0.5",
    "typescript": "4.5.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "./addon-main.cjs",
    "app-js": {
      "./helpers/find-all.js": "./dist/_app_/helpers/find-all.js",
      "./helpers/find-record.js": "./dist/_app_/helpers/find-record.js",
      "./helpers/query-record.js": "./dist/_app_/helpers/query-record.js",
      "./helpers/query.js": "./dist/_app_/helpers/query.js"
    }
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "volta": {
    "extends": "../package.json"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "peerDependencies": {
    "ember-data": "^3.25.0"
  },
  "types": "dist"
}
