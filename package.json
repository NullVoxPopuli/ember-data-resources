{
  "name": "ember-data-resources",
  "version": "4.0.4",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "workspaces": [
    "ember-data-resources",
    "test-app"
  ],
  "scripts": {
    "build": "yarn workspace ember-data-resources run build",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "prepare": "yarn build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "yarn workspace ember-data-resources run start",
    "start:test": "yarn workspace test-app run start",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "prettier": "^2.7.1"
  }
}
